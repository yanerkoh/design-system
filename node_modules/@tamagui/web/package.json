{
  "name": "@tamagui/web",
  "version": "1.138.3",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "types": "./types/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Nate Wienert"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "files": [
    "types",
    "dist",
    "src",
    "reset.css"
  ],
  "dependencies": {
    "@tamagui/compose-refs": "1.138.3",
    "@tamagui/constants": "1.138.3",
    "@tamagui/helpers": "1.138.3",
    "@tamagui/is-equal-shallow": "1.138.3",
    "@tamagui/normalize-css-color": "1.138.3",
    "@tamagui/timer": "1.138.3",
    "@tamagui/types": "1.138.3",
    "@tamagui/use-did-finish-ssr": "1.138.3",
    "@tamagui/use-event": "1.138.3",
    "@tamagui/use-force-update": "1.138.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@tamagui/build": "1.138.3",
    "@testing-library/react": "^16.1.0",
    "csstype": "^3.0.10",
    "react": "*",
    "react-dom": "*",
    "react-native": "0.81.5",
    "typescript": "~5.9.2",
    "vitest": "^4.0.4"
  },
  "exports": {
    "./package.json": "./package.json",
    "./reset.css": "./reset.css",
    ".": {
      "react-native": {
        "import": "./dist/esm/index.native.js",
        "require": "./dist/cjs/index.native.js"
      },
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/cjs/index.native.js"
    },
    "./inject-styles": {
      "types": "./types/inject-styles.d.ts",
      "import": "./dist/esm/inject-styles.mjs",
      "require": "./dist/cjs/inject-styles.cjs",
      "default": "./dist/cjs/inject-styles.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tamagui/tamagui.git",
    "directory": "packages/web"
  },
  "publishConfig": {
    "access": "public"
  }
}